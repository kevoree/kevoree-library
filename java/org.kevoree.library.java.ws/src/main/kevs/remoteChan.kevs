add node0, node1 : JavaNode
add node1.printer : Calculator
add node0.ticker : CalculatorInvoker
add sync : WSGroup
add chan : RemoteWSChan/5.1.4-SNAPSHOT

set chan.host = "localhost"
set chan.port = "9050"
set sync.master = "node0"

bind node0.ticker.twoParams chan
bind node1.printer.toJson chan

attach node0, node1 sync
