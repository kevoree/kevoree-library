add node0 : JavaNode
add node0.ticker : Ticker
add sync : WSGroup
add chan : RemoteWSChan

set sync.master = 'node0'
set sync.onConnect = 'add {nodeName}.printer: ConsolePrinter
bind {nodeName}.printer.input chan'

set sync.onDisconnect = 'remove {nodeName}'

set chan.host = 'ws.kevoree.org'
set chan.uuid = 'wsgroup-test'

attach node0 sync

bind node0.ticker.tick chan

set node0.log = 'debug'