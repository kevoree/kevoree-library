add node0, node1, node2 : JavaNode
add node0.log, node1.log, node2.log : ConsolePrinter
add node1.ticker : Ticker
add sync : WSGroup
add broker : MQTTChannel/5.0.8-SNAPSHOT

attach node0, node1, node2 sync

set sync.master = "node0"

bind node0.log.input broker
//bind node1.log.input broker
bind node2.log.input broker
bind node1.ticker.tick broker